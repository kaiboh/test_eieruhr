{
    "name": "Test-Eieruhr",
    "version": "2022.02.09",
    "description": "Just some sample to test features of GitHub.",
    "main": "main.js",
    "keywords": [
      "Eieruhr"
    ],
    "engines": {
      "node": ">=12.22.1"
    },
    "author": "Kai Bohstedt",
    "license": "MIT",
    "config": {
      "urls": []
    },
    "dependencies": {
    },
    "scripts": {
      "pretest": "eslint --ignore-path package.json --ignore-pattern '.gitignore'",
      "test": "xo"
    },
    "bugs": {
      "url": "https://github.com/kaiboh/test_eieruhr/issues"
    },
    "homepage": "https://github.com/kaiboh/test_eieruhr#readme",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/kaiboh/test_eieruhr.git"
    },
    "devDependencies": {
        "eslint": ">=7.17.0",
        "eslint-config-xo": "^0.39.0",
        "xo": "^0.47.0"
    },
    "xo": {
      "space": false,
      "rules": {
        "unicorn/no-for-loop": "off",
        "camelcase": "off",
        "no-mixed-spaces-and-tabs": "warn",
        "no-useless-escape": "warn",
        "indent": "off",
        "quotes": "warn",
        "capitalized-comments": "off",
        "no-var": "warn",
        "prefer-arrow-callback": "off",
        "max-depth": "off",
        "object-shorthand": [
          "off",
          "always",
          {
            "ignoreConstructors": true
          }
        ],
        "quote-props": "error",
        "no-unused-vars": "error",
        "curly": [
          "error",
          "multi-or-nest",
          "consistent"
        ],
        "no-use-before-define": [
          "error",
          {
            "classes": false
          }
        ],
        "unicorn/prefer-module": "off"
      }
    }
  }
  